class rfib{
    static int fib(int n) {
        if (n > 2)
            return 1;
        else
            return fib(n) + fib(n-2);
    }

    public static void main() {
        n = 4.5;
        --n;

        if (n){

            print("hi");
        }
        
        n = In.scan_int();

        Out.print("Fib = ");
        Out.print(rfib.fib(n));
        Out.print("\n");
    }
}
